var done = false
var can = true

func _on_Exit_body_entered(body):
	if body.is_in_group(\"Anton\") and can:
		can = false
		if !done:
			if body.state == 0:
				done = true
				get_parent().get_node(\"Manager\").to_secret()
				
		else:
			if body.state == 0:
				done = false
				get_parent().get_node(\"Manager\").from_secret()
		
		
