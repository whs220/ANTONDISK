script/source = "extends Area2D

export (int) var which = 0
var is_ready = false

func spawn():
	is_ready = true
	$AnimationPlayer.play(\"treasure\")

func _on_Treasure_body_entered(body):
	if body.is_in_group(\"Anton\") and is_ready:
		is_ready = false
		body.play_wow()
		collect()

func collect():
	$AudioStreamPlayer.play()
	Globals.add_score(5000)
	Globals.set(\"treasure_\" + str(which), true)
	$AnimationPlayer.play(\"collect\")
	yield($AnimationPlayer, \"animation_finished\")
	queue_free()
"

