resource_name = "CameraBumpers"
script/source = "extends Area2D

export (bool) var on_x = true
export (int) var new_int = 1
var bodies = []

var snd = true

func _on_Bumpers_body_entered(body):
	if !bodies.has(body):
		bodies.append(body)

func _physics_process(delta):
	if bodies.size() > 0:
		for body in bodies:
			if body.is_in_group(\"Ball\"):
				if body.state != 0:
					body.get_node(\"AnimationPlayer\").play(\"hit\")
					get_parent().set_trauma(0.15)
					if on_x:
						body.velocity.x = new_int
						body.velocity = body.velocity.normalized()
						play_snd(body)
					else:
						body.velocity.y = new_int
						body.velocity = body.velocity.normalized()
						play_snd(body)
			if body.is_in_group(\"Disk\"):
				if on_x:
					body.disk_dir.x = new_int
				else:
					body.disk_dir.y = new_int

func _on_Bumpers_body_exited(body):
	if bodies.has(body):
		bodies.erase(body)

func play_snd(b):
	if snd and b.state != 0:
		snd = false
		b.get_node(\"AudioStreamPlayer\").play()
		yield(get_tree().create_timer(0.5, false), \"timeout\")
		snd = true
"

