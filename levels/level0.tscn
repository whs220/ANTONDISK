resource_name = "BruloPillar"
script/source = "extends Area2D

export (bool) var can_be_disced
var done = false

func _ready():
	$AnimatedSprite.play(\"idle\")

func _on_BruloPillar_body_entered(body):
	if body.is_in_group(\"Anton\") and !can_be_disced:
		go()
	elif can_be_disced and body.is_in_group(\"Disk\"):
		go()

func go():
	if !done:
		done = true
		$AnimatedSprite.play(\"active\")
		var rng = RandomNumberGenerator.new()
		rng.randomize()
		
		var snd = rng.randi_range(1,2)
		$AudioStreamPlayer.stream = load(\"res://sfx/snd_brulo_yay\" + str(snd) + \".wav\")
		$AudioStreamPlayer.play()
		
		get_parent().get_node(\"Manager\").progress()
	
"

