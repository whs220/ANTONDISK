script/source = "extends Area2D

var done = false
onready var anton = get_tree().get_nodes_in_group(\"Anton\")[0]

func _on_Phase3_body_entered(body):
	if body.is_in_group(\"Anton\"):
		trans()

func trans():
	if !done:
		done = true
		get_parent().get_node(\"Camera2D/AnimationPlayer\").play(\"path2\")
		get_parent().get_node(\"Camera2D/PlayerWalls/CollisionShape2D4\").queue_free()
		anton.state = 2
		anton.global_position = Vector2(816, 2016)
		anton.get_node(\"Sprite\").play(\"lookingdown\")
		yield(get_tree().create_timer(4, false), \"timeout\")
		anton.state = 0
		anton.off_screen_die = true
		get_parent().get_node(\"Ball\").launch(Vector2(0.5, -0.5), 400)
		yield(get_parent().get_node(\"Camera2D/AnimationPlayer\"), \"animation_finished\")
		get_parent().get_node(\"Camera2D/Fireholder/AnimationPlayer\").play(\"fire\")
		get_parent().get_node(\"Camera2D/AnimationPlayer\").play(\"path3\")
		get_parent().get_node(\"Camera2D/Fireholder/FIRE\").enabled = true
		get_parent().get_node(\"Fist\")._start()
"

