script/source = "extends Area2D

onready var flame = preload(\"res://objs/Flame.tscn\")

func _on_Lava_body_entered(body):
	if body.is_in_group(\"Ball\"):
		spawn(Vector2(body.global_position.x, 180))

func spawn(pos):
	var obj1 = flame.instance()
	obj1.global_position = pos
	get_parent().add_child(obj1)
	obj1.start(1)
	
	var obj2 = flame.instance()
	obj2.global_position = pos
	get_parent().add_child(obj2)
	obj2.start(-1)
