[sub_resource type="GDScript" id=136]
resource_name = "PunchHand"
script/source = "extends Area2D

var bricks = []
var anton = null

var punches = 0

func _start():
	$Timer.start()

func _on_Timer_timeout():
	self.global_position = get_parent().get_node(\"Anton\").global_position
	$AnimationPlayer.play(\"punch\")

func punch():
	punches += 1
	print(punches)
	for brick in bricks:
		brick.destroy(false, false, null, false, true, self)
	if anton != null:
		anton.take_damage(self.global_position)
	
	
	match punches:
		8:
			$Timer.wait_time = 2
		15:
			$Timer.wait_time = 1
	$Timer.start()
	


func _on_Fist_body_entered(body):
	if body.is_in_group(\"Brick\"):
		bricks.append(body)
	if body.is_in_group(\"Anton\"):
		anton = body

func _on_Fist_body_exited(body):
	if bricks.has(body):
		bricks.erase(body)
	if body.is_in_group(\"Anton\"):
		anton = null

func stop():
	queue_free()
"

