resource_name = "Satan"
script/source = "extends Node2D

var time_between_attacks = 3
var speed = 1
onready var anton = get_tree().get_nodes_in_group(\"Anton\")[0]
onready var camera = get_parent().get_node(\"Camera2D\")
onready var ui = get_parent().get_node(\"UI\")

var physic_attack = -1

var phase_hp = 15
var animing = false

var phase = 1

func take_damage(dam):
	if !animing:
		get_parent().get_node(\"Camera2D\").add_trauma(0.2)
		phase_hp -= dam
		if phase_hp <= 0:
			if phase == 1:
				$AudioStreamPlayer.play()
				anton.play_happy_sfx()
				phase += 1
				get_parent().get_node(\"EvilBall\")._on_Timer_timeout()
				get_parent().get_node(\"Ball\").disable()
				physic_attack = -1
				$Head.follow = false
				animing = true
				$AnimationPlayer.stop()
				$Head/AnimationPlayer.stop()
				$Tween.stop_all()
				$Timer.stop()
				$LeftHand/CollisionShape2D.disabled = true
				$RightHand/CollisionShape2D.disabled = true
				$Head/AnimationPlayer.play(\"phase2_intro\")
				physic_attack = -1
				yield($Head/AnimationPlayer, \"animation_finished\")
				$Head.follow = true
				$Head/AnimationPlayer.play(\"idle\")
				$LeftHand/CollisionShape2D.disabled = false
				$RightHand/CollisionShape2D.disabled = false
				animing = false
				
				
				speed = 1.2
				$Head/AnimationPlayer.playback_speed = 1.2
				time_between_attacks = 2
				start()
				phase_hp = 15
			elif phase == 2:
				$AudioStreamPlayer.play()
				anton.play_happy_sfx()
				#get_parent().get_node(\"Manager\").destroy_tilemap() # take out laetr
				$Head/AnimationPlayer.playback_speed = 1
				get_parent().get_node(\"EvilBall\")._on_Timer_timeout()
				speed = 1
				phase += 1
				get_parent().get_node(\"Ball\").disable()
				anton.off_screen_die = false
				physic_attack = -1
				$Head.follow = false
				animing = true
				$AnimationPlayer.stop()
				$Head/AnimationPlayer.stop()
				$Tween.stop_all()
				$Timer.stop()
				$LeftHand/CollisionShape2D.disabled = true
				$RightHand/CollisionShape2D.disabled = true
				$Head/AnimationPlayer.play(\"phase3_intro\")
				physic_attack = -1
				yield($Head/AnimationPlayer, \"animation_finished\")
				anton.velocity.y = -1600
				get_parent().get_node(\"EnemyOnly\").queue_free()
				get_parent().get_node(\"Lava\").queue_free()
		else:
			var rng = RandomNumberGenerator.new()
			$Hit.pitch_scale = rng.randf_range(0.6, 0.8)
			$Hit.play()

func start():
	$Head/AnimationPlayer.play(\"idle\")
	idle()

func idle():
	$Timer.wait_time = time_between_attacks
	$AnimationPlayer.playback_speed = speed
	$AnimationPlayer.play(\"idle\")
	$Timer.start()

func fistbump():
	$AnimationPlayer.play(\"fistbump\")

func hook():
	$AnimationPlayer.play(\"right_swing\")

func smash():
	if animing:
		physic_attack = -1
		return
	$AnimationPlayer.stop()
	physic_attack = 0
	yield(get_tree().create_timer(2/speed, false), \"timeout\")
	if animing:
		physic_attack = -1
		return
	physic_attack = -1
	$Tween.interpolate_property($LeftHand, \"global_position\", $LeftHand.global_position, Vector2($LeftHand.global_position.x, 132), 0.3, Tween.TRANS_ELASTIC, Tween.EASE_IN)
	$Tween.start()
	yield($Tween, \"tween_completed\")
	if animing:
		physic_attack = -1
		return
	camera.add_trauma(1)
	$Slam.play()
	yield(get_tree().create_timer(1, false), \"timeout\")
	if animing:
		physic_attack = -1
		return
	$Tween.interpolate_property($LeftHand, \"global_position\", $LeftHand.global_position, Vector2(1020, -60), 1.5/speed, Tween.TRANS_ELASTIC, Tween.EASE_OUT)
	$Tween.start()
	$LeftHand/CollisionShape2D.disabled = true
	#$RightHand/CollisionShape2D.disabled = true
	yield($Tween, \"tween_completed\")
	if animing:
		physic_attack = -1
		return
	$LeftHand/CollisionShape2D.disabled = false
	#$RightHand/CollisionShape2D.disabled = false
	idle()

func double_smash():
	if animing:
		physic_attack = -1
		return
	$AnimationPlayer.stop()
	physic_attack = 0
	$RightHand/Sprite.flip_h = false
	$RightHand/Sprite.play(\"point\")
	yield(get_tree().create_timer(2/speed, false), \"timeout\")
	if animing:
		physic_attack = -1
		return
	$RightHand/Sprite.flip_h = true
	$RightHand/Sprite.play(\"fist\")
	physic_attack = 1
	$Tween.interpolate_property($LeftHand, \"global_position\", $LeftHand.global_position, Vector2($LeftHand.global_position.x, 132), 0.3, Tween.TRANS_ELASTIC, Tween.EASE_IN)
	$Tween.start()
	yield($Tween, \"tween_completed\")
	if animing:
		physic_attack = -1
		return
	camera.add_trauma(1)
	$Slam.play()
	yield(get_tree().create_timer(1/speed, false), \"timeout\")
	if animing:
		physic_attack = -1
		return
	physic_attack = -1
	$Tween.interpolate_property($RightHand, \"global_position\", $RightHand.global_position, Vector2($RightHand.global_position.x, 132), 0.3, Tween.TRANS_ELASTIC, Tween.EASE_IN)
	$Tween.start()
	yield($Tween, \"tween_completed\")
	if animing:
		physic_attack = -1
		return
	camera.add_trauma(1)
	$Slam.play()
	yield(get_tree().create_timer(1, false), \"timeout\")
	if animing:
		physic_attack = -1
		return
	$Tween.interpolate_property($LeftHand, \"global_position\", $LeftHand.global_position, Vector2(1020, -60), 1.5/speed, Tween.TRANS_ELASTIC, Tween.EASE_OUT)
	$Tween.interpolate_property($RightHand, \"global_position\", $RightHand.global_position, Vector2(608, -60), 1.5/speed, Tween.TRANS_ELASTIC, Tween.EASE_OUT)
	$LeftHand/CollisionShape2D.disabled = true
	$RightHand/CollisionShape2D.disabled = true
	$Tween.start()
	yield($Tween, \"tween_completed\")
	if animing:
		physic_attack = -1
		return
	$LeftHand/CollisionShape2D.disabled = false
	$RightHand/CollisionShape2D.disabled = false
	idle()

func evil_ball():
	$AnimationPlayer.play(\"summon\")
	get_parent().get_node(\"EvilBall\").global_position = Vector2(816, -20)

func _physics_process(delta):
	match physic_attack:
		0:
			$LeftHand.global_position = $LeftHand.global_position.linear_interpolate(anton.global_position + Vector2(0,-192), 0.1)
		1:
			$RightHand.global_position = $RightHand.global_position.linear_interpolate(anton.global_position + Vector2(0,-192), 0.1)

func _on_Timer_timeout():
	if !animing:
		## ATTACK!
		var rng = RandomNumberGenerator.new()
		rng.randomize()
		var num = 0
		var good = false
		while (!good):
			good = true
			num = rng.randi_range(0,2)
			if num == 3 and get_parent().get_node(\"EvilBall\").enabled:
				good = false
				## ball is still active, reroll
		
		match num:
			0:
				fistbump()
			1:
				hook()
			2:
				if phase == 2:
					double_smash()
				else:
					smash()
			3:
				evil_ball()
		
		yield($AnimationPlayer, \"animation_finished\")
		idle()

#func _on_Anton_damage():
#	if ui.hp == 1:
#		$Head/Mouth.play(\"laugh\")
#		$Head/Eyes.play(\"lowhp\")

func grow_ball():
	get_parent().get_node(\"Ball\").grow(Vector2(816, -20))
"

