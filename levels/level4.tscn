[sub_resource type="GDScript" id=140]
resource_name = "Finale"
script/source = "extends StaticBody2D

var hits = 0
onready var fh = get_parent().get_node(\"FinalHead\")

func start():
	fh.get_node(\"AnimationPlayer\").play(\"intro\")
	yield(fh.get_node(\"AnimationPlayer\"), \"animation_finished\")
	fh.get_node(\"AnimatedSprite\").play(\"worried\")
	fh.get_node(\"AnimationPlayer\").play(\"worried\")

func destroy(ex=false, low_t=false, b=null, from_ex = false, shake=true, s=null):
	if s != get_parent().get_node(\"Fist\"):
		hits += 1
		get_parent().get_node(\"Music\").pitch_scale += 0.1
		get_parent().get_node(\"Camera2D\").add_trauma(1)
		
		match hits:
			1:
				$AudioStreamPlayer.play()
				$Sprite.frame += 1
			2:
				$AudioStreamPlayer.stream = preload(\"res://sfx/tnt_hit2.ogg\")
				$AudioStreamPlayer.play()
				$Sprite.frame += 1
			3:
				$AudioStreamPlayer.stream = preload(\"res://sfx/tnt_hit3.ogg\")
				$AudioStreamPlayer.play()
				$Sprite.frame += 1
			4:
				$AudioStreamPlayer.stream = preload(\"res://sfx/tnt_hit4.ogg\")
				$AudioStreamPlayer.play()
				$Sprite.frame += 1
			5:
				$AudioStreamPlayer.stream = preload(\"res://sfx/tnt_hit5.ogg\")
				$AudioStreamPlayer.play()
				$Sprite.frame += 1
			6:
				$AudioStreamPlayer.stream = preload(\"res://sfx/tnt_hit6.ogg\")
				$AudioStreamPlayer.play()
				$Sprite.frame += 1
				finish()

func finish():
	Globals.beat_game = true
	
	get_parent().get_node(\"Anton\").off_screen_die = false
	get_parent().get_node(\"Fist\").stop()
	fh.get_node(\"AnimationPlayer\").play(\"end\")
	get_parent().get_node(\"Pause\").can_pause = false
	get_parent().get_node(\"Music\").stop()
	get_parent().get_node(\"Ball\").disable()
	get_parent().get_node(\"Anton\").state = 2
	get_parent().get_node(\"Anton\").anim_override = true
	get_parent().get_node(\"Anton/Sprite\").play(\"lookingdown\")
	$AnimationPlayer.play(\"finish\")
	yield(get_tree().create_timer(7, false), \"timeout\")
	get_parent().get_node(\"Anton\").anim_override = false
	get_parent().get_node(\"Anton\").celebrate()
	yield(get_tree().create_timer(3.65, false), \"timeout\")
	get_parent().get_node(\"CanvasLayer/AnimationPlayer\").play(\"final\")
	yield(get_tree().create_timer(3, false), \"timeout\")
	Transitions.grow_out()
	yield(Transitions, \"done\")
	yield(get_tree().create_timer(3, false), \"timeout\")
	get_tree().change_scene(\"res://levels/ending_cutscene.tscn\")
"

