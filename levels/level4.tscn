resource_name = "Main"
script/source = "extends Node2D

var ball_speed = 400

var old_bricks = 0
var old_score = 0

var total_bricks = 0

var texts = false

var back = false
var amount = 400

func _ready():
	get_tree().paused = false
	Transitions.grow_in()
	$UI/Main/TimeBonus/AnimationPlayer.play(\"RESET\")
	$UI.show()
	set_physics_process(false)
	VisualServer.set_default_clear_color(Color(0, 0, 0))
	if !Globals.checkpoint:
		walk_in()
		Globals.checkpoint = true
	else:
		quick_load()

func quick_load():
	$Anton.off_screen_die = false
	$Anton.state = 2
	$Anton.anim_override = true
	$Anton/Sprite.play(\"lookat\")
	$ParallaxBackground/AnimationPlayer.play(\"fade\")
	$Camera2D/AnimationPlayer.play(\"path1\")
	$Camera2D/AnimationPlayer.seek(12)
	$Anton.global_position = Vector2(816, 192)
	$Music.stream = load(\"res://music/final_boss.ogg\")
	$Music.play()
	$SATAN/Head/AnimationPlayer.play(\"intro\")
	yield(get_tree().create_timer(2.67), \"timeout\")
	$Anton.anim_override = true
	$Anton.state = 0
	$UI/AnimationPlayer.play(\"intro\")
	$UI/Intro/AnimationPlayer.play(\"intro_fast\")
	$Ball.launch(Vector2(0.5, 0.5), 6.5)
	$Pause.can_pause = true ## NEW!
	yield($SATAN/Head/AnimationPlayer, \"animation_finished\")
	$SATAN/LeftHand/CollisionShape2D.disabled = false
	$SATAN.start()

func finish():
	$Anton.off_screen_die = false
	$Camera2D/AnimationPlayer.stop(false)
	$Music/Tween.interpolate_property($Music, \"volume_db\", 0, -80, 3, Tween.TRANS_LINEAR)
	$Music/Tween.start()
	$Anton.state = 2
	yield($Music/Tween, \"tween_completed\")
	$Music/Tween.interpolate_property($Camera2D, \"global_position\", $Camera2D.global_position, $Anton.global_position, 1, Tween.TRANS_SINE, Tween.EASE_IN_OUT)
	$Music/Tween.start()
	yield($Music/Tween, \"tween_completed\")
	$Anton.celebrate()
	$Win.play()
	yield(get_tree().create_timer(1), \"timeout\")
	texts = true
	set_physics_process(true)
	$UI/Goal/AnimationPlayer.play(\"show_stats\")
	yield(get_tree().create_timer(1), \"timeout\")
	$Anton.laugh = false
	yield($UI/Goal/AnimationPlayer, \"animation_finished\")
	texts = false
	$Camera2D/PlayerWalls.queue_free()

func show_bricks():
	$UI/Goal/Tween.interpolate_property(self, \"old_bricks\", old_bricks, total_bricks, 1, Tween.TRANS_EXPO, Tween.EASE_OUT)
	$UI/Goal/Tween.start()

func show_score():
	$UI/Goal/Tween.interpolate_property(self, \"old_score\", old_score, $UI.score, 1, Tween.TRANS_EXPO, Tween.EASE_OUT)
	$UI/Goal/Tween.start()

func _physics_process(delta):
	if !texts:
		if !back:
			$Anton.velocity = Vector2(amount,0)
		else:
			$Anton.velocity = Vector2(-amount,0)
	else:
		$UI/Goal/Bricks.bbcode_text = \"[center]Bricks Broken: [color=yellow]\" + str(floor(old_bricks))
		$UI/Goal/Bricks2.bbcode_text = \"[center]Final Score: [color=yellow]\" + str(floor(old_score))

func walk_in():
	$SATAN/LeftHand/CollisionShape2D.disabled = true
	$UI/BlackBars/AnimationPlayer.play(\"in\")
	for wall in $Camera2D/PlayerWalls.get_children():
		wall.disabled = true
	$Anton.state = 2
	set_physics_process(true)
	yield(get_tree().create_timer(2), \"timeout\")
	$Camera2D/AnimationPlayer.play(\"path1\")
	yield(get_tree().create_timer(2.5), \"timeout\")
	set_physics_process(false)
	yield(get_tree().create_timer(2), \"timeout\")
	$Anton.celebrate()
	yield(get_tree().create_timer(1), \"timeout\")
	$Anton.laugh = false
	amount=120
	set_physics_process(true)
	yield(get_tree().create_timer(4.2), \"timeout\")
	set_physics_process(false)
	$Anton.shock()
	yield(get_tree().create_timer(1.5), \"timeout\")
	amount = 400
	$Anton.shock = false
	back = true
	set_physics_process(true)
	yield(get_tree().create_timer(0.4), \"timeout\")
	set_physics_process(false)
	yield(get_tree().create_timer(1), \"timeout\")
	$Anton.velocity = Vector2(0.1,0)
	yield(get_tree().create_timer(0.5), \"timeout\")
	$Anton.velocity = Vector2(-0.1,0)
	yield(get_tree().create_timer(0.2), \"timeout\")
	$ParallaxBackground/AnimationPlayer.play(\"fade\")
	$Anton.velocity = Vector2(0.1,0)
	yield(get_tree().create_timer(0.2), \"timeout\")
	$Anton.velocity = Vector2(-0.1,0)
	yield(get_tree().create_timer(0.1), \"timeout\")
	$Anton.velocity = Vector2(0.1,0)
	yield(get_tree().create_timer(0.1), \"timeout\")
	$Anton.velocity = Vector2(-0.1,0)
	for wall in $Camera2D/PlayerWalls.get_children():
		wall.disabled = false
	yield(get_tree().create_timer(1), \"timeout\")
	$Music.stop()
	$Music.stream = load(\"res://music/final_boss.ogg\")
	$Music.play()
	$Anton.anim_override = true
	$Anton/Sprite.play(\"lookat\")
	$SATAN/Head/AnimationPlayer.play(\"intro\")
	yield(get_tree().create_timer(2.67), \"timeout\")
	$Anton.anim_override = false
	$UI/BlackBars/AnimationPlayer.play(\"out\")
	$Anton.state = 0
	$UI/AnimationPlayer.play(\"intro\")
	$UI/Intro/AnimationPlayer.play(\"intro_fast\")
	$Ball.launch(Vector2(0.5, 0.5), 6.5)
	$Pause.can_pause = true ## NEW!
	yield($SATAN/Head/AnimationPlayer, \"animation_finished\")
	$SATAN/LeftHand/CollisionShape2D.disabled = false
	$SATAN.start()
"

