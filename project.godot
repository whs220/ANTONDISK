config_version=4

[application]

config/name="ANTONDISK"
config/description="Game made for ANTONJAM 3 (2022)!"
run/main_scene="res://levels/Menu.tscn"
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://icon.png"
config/windows_native_icon="res://icon.ico"

[audio]

output_latency=60

[autoload]

SoundManager="res://addons/sound_manager/module/SoundManager.tscn"
Globals="*res://globals.gd"
Transitions="*res://objs/Transitions.tscn"
Screenshots="res://screenshots.gd"

[display]

window/size/width=800
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray(  )

[global]

pixel=false

[gui]

common/drop_mouse_on_gui_input_disabled=true

